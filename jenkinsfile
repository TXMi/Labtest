pipeline{
    agent any
    stages{
        stage('Checkout'){
            steps{
                echo 'Successfully Checkout'
            }
        }
        stage('Dependency Check'){
            steps{
                dependencyCheck additionalArguments: '--format HTML --format XML --suppresion suppresion.xml', odcInstallation: 'Default'
            }
        }
        stage('Deploy'){
            steps{
                echo 'Deploying...'
            }
        }
    }
}